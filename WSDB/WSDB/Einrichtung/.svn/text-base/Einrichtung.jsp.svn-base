<table width="100%"  border="0" cellspacing="0" cellpadding="0">
	  <tr>
		<td colspan="6" class="bottom"><div align="left">neue Einrichtung erfassen: </div></td>
	  </tr>
	  <tr>
		<td colspan="3"><div align="center">&nbsp;</div></td>
		<td colspan="3"><div align="center">&nbsp;</div></td>
	  </tr>	  
	  <tr>
		<td width="24%"><div align="right">Name der Einrichtung:</div></td>
		<td width="2%"><div align="center">*</div></td>
		<td colspan="4"><div align="left">
			<html:text 	name="wsForm"
						property="einrichtung.name" 
						titleKey="label.Vorname.title" 
						onfocus="this.style.backgroundColor='#C5D4E0'" 
						onblur="this.style.backgroundColor='#FFFFFF'"/>		
		</div></td>
	  </tr>
	  <tr>
		<td colspan="3"><div align="center"></div></td>
		<td colspan="3"><div align="center"></div></td>
	  </tr>	 
	  <tr>
		<td width="24%"><div align="right">Strasse:</div></td>
		<td width="2%"><div align="center">&nbsp;</div></td>
		<td width="24%">
			<html:text 	name="wsForm"
						property="einrichtung.adresse.strasse" 
						titleKey="label.Nachname.title" 
						onfocus="this.style.backgroundColor='#C5D4E0'" 
						onblur="this.style.backgroundColor='#FFFFFF'"/>		
		</td>
		<td width="24%"><div align="right">Hausnummer:</div></td>
		<td width="2%">&nbsp;</td>
		<td width="24%">
			<html:text 	name="wsForm"
						property="einrichtung.adresse.hausnummer" 
						titleKey="label.Nachname.title" 
						onfocus="this.style.backgroundColor='#C5D4E0'" 
						onblur="this.style.backgroundColor='#FFFFFF'"/>			
		</td>
	  </tr>
	  <tr>
		<td colspan="3"><div align="center"></div></td>
		<td colspan="3"><div align="center"></div></td>
	  </tr>	
	  <tr>
		<td width="24%"><div align="right">Postleitzahl:</div></td>
		<td width="2%"><div align="center">&nbsp;</div></td>
		<td width="24%">
			<html:text 	name="wsForm"
						property="einrichtung.adresse.postleitzahl" 
						titleKey="label.Nachname.title" 
						onfocus="this.style.backgroundColor='#C5D4E0'" 
						onblur="this.style.backgroundColor='#FFFFFF'"/>		
		</td>
		<td width="24%"><div align="right">Ort:</div></td>
		<td width="2%">&nbsp;</td>
		<td width="24%">
			<html:text 	name="wsForm"
						property="einrichtung.adresse.ort" 
						titleKey="label.Nachname.title" 
